<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="pt.mobiledev.tvalarmes"
          android:versionCode="1"
          android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="9" android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
   
    <application android:allowBackup="true"
                 android:icon="@drawable/ic_launcher"
                 android:label="@string/app_name"
                 android:theme="@style/AppTheme" >
        <activity android:name="pt.mobiledev.tvalarmes.AlarmsActivity" android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="pt.mobiledev.tvalarmes.ChannelsActivity"/>
        <activity android:name="pt.mobiledev.tvalarmes.ProgramsActivity"/>
        <receiver android:name="pt.mobiledev.tvalarmes.util.AlarmReceiver" >
    </receiver>
    </application>
    
   
    

</manifest>
